        // Call logRecord(ENUM_LOG_LEVEL_INFO, "¼ÓÔØµØÍ¼ÖÐ")
        
    Case ENUM_STATUS_HAS_GHOST_TASK
        // Call logRecord(ENUM_LOG_LEVEL_INFO, "×½¹íÖÐ")
        
    Case ENUM_STATUS_HAS_GHOSTKING_TASK
