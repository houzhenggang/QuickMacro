    Case ENUM_STATUS_HAS_OTHER_CONFIRM
    	If TRUE <> cancelOtherConfirms(hWnd, left, top, right, bottom) Then 
            handlerGhostShortStatus = RET_FAILED
            Exit Function
        End If
        
    Case Else
        //Call logRecord(ENUM_LOG_LEVEL_INFO, "Î´Öª")
        
